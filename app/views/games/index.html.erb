<section class="container">

<h1>Flash Game List</h1>
<div class="container">
	<div class="thumbnails">
	<div class="row visable-sm visable-lg">
		<% @games.each do |game| %>
			<div class="col-xs-1 col-md-3">
				 <div class="img-thumbnail">
					<div><a href="<%= game_path(game) %>"><%= image_tag game.image_url(:thumb).to_s %></a>
				        <div><%= game.title %></div>
					    <div class="action"><%= link_to "edit", edit_game_path(game) %></div>
					</div>
				 </div>
			</div>
		<% end %>
	</div>
	</div>
</div>
</section>